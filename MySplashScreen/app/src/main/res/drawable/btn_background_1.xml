<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <rotate
            android:fromDegrees="45"
            android:toDegrees="0"
            android:pivotX="140%"
            android:pivotY="13%">
            <shape android:shape="rectangle">
                <corners android:radius="50dp"/>
                <solid android:color="#ffffff" />
            </shape>
        </rotate>
    </item>

